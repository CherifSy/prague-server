.row
  = breadcrumbs [[current_organization.name, organization_path(current_organization)],
                 ['Users']]

  .col-lg-12
    = title "User Accounts"

    %table.table
      %thead
        %th Email
        %th Actions

      - @users.each do |user|
        %tr
          %td= user.email
          %td
            - if user != current_user
              = link_to 'Delete', organization_user_path(current_organization, user), method: :delete, class: 'btn btn-warning'
.row
  .col-lg-12
    #invitation-status
    #invitation-form
      = render partial: 'org/invitations/form', locals: { invitation: @invitation}
